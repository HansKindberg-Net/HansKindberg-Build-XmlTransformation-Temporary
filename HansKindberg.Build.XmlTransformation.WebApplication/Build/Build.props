<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<!-- High, Normal or Low -->
		<XmlTransformationLogImportance Condition="$(XmlTransformationLogImportance) == ''">High</XmlTransformationLogImportance>
	</PropertyGroup>
	<ItemGroup>
		<XmlTransformationMap Include="Web.config">
			<Source>$(SolutionDir)packages\SomePackage\Configuration\Web.Template.config</Source>
		</XmlTransformationMap>
	</ItemGroup>
</Project>